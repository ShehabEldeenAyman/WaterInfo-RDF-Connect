@base <http://example.com/> .
@prefix rr:  <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql:  <http://semweb.mmlab.be/ns/ql#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex:  <http://example.com/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfc: <https://w3id.org/rdf-connect#> .

<#SensorMapping> a rr:TriplesMap ;
  rml:logicalSource [
    #rml:source "sample_data.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;

  rr:subjectMap [
    rr:template "http://example.com/reading_{ts_id}_{Timestamp}"
  ] ;

  # sosa:Observation type
  rr:predicateObjectMap [
    rr:predicate rdf:type ;
    rr:objectMap [ rr:constant sosa:Observation ]
  ] ;

  # sosa:madeBySensor → ts_id
  rr:predicateObjectMap [
    rr:predicate sosa:madeBySensor ;
    rr:objectMap [ rml:reference "ts_id" ]
  ] ;

  # sosa:hasSimpleResult → Value (xsd:double)
  rr:predicateObjectMap [
    rr:predicate sosa:hasSimpleResult ;
    rr:objectMap [
      rml:reference "Value" ;
      rr:datatype xsd:double
    ]
  ] ;

  # sosa:observedProperty → stationparameter_longname
  rr:predicateObjectMap [
    rr:predicate sosa:observedProperty ;
    rr:objectMap [ rml:reference "stationparameter_longname" ]
  ] ;

  # sosa:resultTime → Timestamp (xsd:dateTime)
  rr:predicateObjectMap [
    rr:predicate sosa:resultTime ;
    rr:objectMap [
      rml:reference "Timestamp" ;
      rr:datatype xsd:dateTime
    ]
  ] .
