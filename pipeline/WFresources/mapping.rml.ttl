@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix dc: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix : <http://mapping.example.com/>.
@prefix ex: <http://example.com/>.
@prefix sosa: <http://www.w3.org/ns/sosa/>.
@prefix ssn: <http://www.w3.org/ns/ssn/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

:rules_000 a void:Dataset;
    void:exampleResource :map_timeseries_000.
:map_timeseries_000 rml:logicalSource :source_000.
:source_000 a rml:LogicalSource;
    rml:source "sample_data.csv";
    rml:referenceFormulation ql:CSV.
:map_timeseries_000 a rr:TriplesMap;
    rdfs:label "timeseries".
:s_000 a rr:SubjectMap.
:map_timeseries_000 rr:subjectMap :s_000.
:s_000 rr:template "http://example.com/reading_{ts_id}_{Timestamp}".
:pom_000 a rr:PredicateObjectMap.
:map_timeseries_000 rr:predicateObjectMap :pom_000.
:pm_000 a rr:PredicateMap.
:pom_000 rr:predicateMap :pm_000.
:pm_000 rr:constant rdf:type.
:pom_000 rr:objectMap :om_000.
:om_000 a rr:ObjectMap;
    rr:constant sosa:Observation;
    rr:termType rr:IRI.
:pom_001 a rr:PredicateObjectMap.
:map_timeseries_000 rr:predicateObjectMap :pom_001.
:pm_001 a rr:PredicateMap.
:pom_001 rr:predicateMap :pm_001.
:pm_001 rr:constant sosa:madeBySensor.
:pom_001 rr:objectMap :om_001.
:om_001 a rr:ObjectMap;
    rml:reference "ts_id";
    rr:termType rr:Literal.
:pom_002 a rr:PredicateObjectMap.
:map_timeseries_000 rr:predicateObjectMap :pom_002.
:pm_002 a rr:PredicateMap.
:pom_002 rr:predicateMap :pm_002.
:pm_002 rr:constant sosa:hasSimpleResult.
:pom_002 rr:objectMap :om_002.
:om_002 a rr:ObjectMap;
    rml:reference "Value";
    rr:termType rr:Literal;
    rr:datatype xsd:double.
:pom_003 a rr:PredicateObjectMap.
:map_timeseries_000 rr:predicateObjectMap :pom_003.
:pm_003 a rr:PredicateMap.
:pom_003 rr:predicateMap :pm_003.
:pm_003 rr:constant sosa:observedProperty.
:pom_003 rr:objectMap :om_003.
:om_003 a rr:ObjectMap;
    rml:reference "stationparameter_longname";
    rr:termType rr:Literal.
:pom_004 a rr:PredicateObjectMap.
:map_timeseries_000 rr:predicateObjectMap :pom_004.
:pm_004 a rr:PredicateMap.
:pom_004 rr:predicateMap :pm_004.
:pm_004 rr:constant sosa:resultTime.
:pom_004 rr:objectMap :om_004.
:om_004 a rr:ObjectMap;
    rml:reference "Timestamp";
    rr:termType rr:Literal;
    rr:datatype xsd:dateTime.
