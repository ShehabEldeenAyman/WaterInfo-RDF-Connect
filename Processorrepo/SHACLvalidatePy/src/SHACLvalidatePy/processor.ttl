@prefix rdfc: <https://w3id.org/rdf-connect#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

rdfc:SHACLvalidatePy rdfc:pyImplementationOf rdfc:Processor;
    rdfs:label "Python Template Processor";
    rdfs:comment "Template processor to kickstart the development of your next Python RDF-Connect processor.";
    rdfc:module_path "SHACLvalidatePy.processor";
    rdfc:class "SHACLvalidatePy".

[ ] a sh:NodeShape;
    sh:targetClass rdfc:SHACLvalidatePy;
    sh:property [
        sh:class rdfc:Reader;
        sh:path rdfc:datareader;
        sh:name "datareader";
        sh:minCount 1;
        sh:maxCount 1;
    ], [
        sh:class rdfc:Writer;
        sh:path rdfc:datawriter;
        sh:name "datawriter";
        sh:minCount 1;
        sh:maxCount 1;
    ], [
        sh:datatype xsd:string;
        sh:path rdfc:loc;
        sh:name "loc";
        sh:minCount 1;
        sh:maxCount 1;
    ], [
        sh:class rdfc:Writer;
        sh:path rdfc:shaclwriter;
        sh:name "shaclwriter";
        sh:minCount 1;
        sh:maxCount 1;
    ].
